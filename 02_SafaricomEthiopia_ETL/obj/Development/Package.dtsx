      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{866FCC75-09F0-4BAF-8999-AD7F8205CA6A}"
          SQLTask:SqlStatementSource="-- 1. Clear existing data&#xA;DELETE FROM DIM_TIME;&#xA;&#xA;-- 2. Define the requested range: July 1, 2021 to January 31, 2026&#xA;DECLARE @StartDate DATE = '2021-07-01';&#xA;DECLARE @EndDate DATE = '2026-01-31';&#xA;&#xA;WHILE @StartDate &lt;= @EndDate&#xA;BEGIN&#xA;    INSERT INTO DIM_TIME (&#xA;        TIME_ID, &#xA;        DATE_TIME, &#xA;        DATE_ONLY, &#xA;        YEAR, &#xA;        QUARTER, &#xA;        MONTH, &#xA;        DAY, &#xA;        HOUR, &#xA;        DAY_OF_WEEK, &#xA;        IS_WEEKEND, &#xA;        IS_BUSINESS_HOUR&#xA;    )&#xA;    SELECT &#xA;        CAST(FORMAT(@StartDate, 'yyyyMMdd') AS INT) AS TIME_ID, -- Result: 20210701&#xA;        CAST(@StartDate AS DATETIME2),&#xA;        @StartDate,&#xA;        YEAR(@StartDate),&#xA;        DATEPART(QUARTER, @StartDate),&#xA;        MONTH(@StartDate),&#xA;        DAY(@StartDate),&#xA;        0, -- Default hour to 0 for daily grain&#xA;        DATENAME(WEEKDAY, @StartDate),&#xA;        CASE WHEN DATEPART(WEEKDAY, @StartDate) IN (1, 7) THEN 'Y' ELSE 'N' END,&#xA;        'N'; -- Can be updated based on 8am-5pm logic if needed&#xA;        &#xA;    SET @StartDate = DATEADD(DAY, 1, @StartDate);&#xA;END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
